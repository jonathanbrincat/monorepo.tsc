{
  "name": "@brincat/vue",
  "version": "0.0.alpha",
  "description": "Vue.js package",
  "author": "Jonathan Brincat <jb@pix8.co.uk>",
  "homepage": "https://github.com/jonathanbrincat/monorepo.tsc#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonathanbrincat/monorepo.tsc.git"
  },
  "bugs": {
    "url": "https://github.com/jonathanbrincat/monorepo.tsc/issues"
  },
  "license": "UNLICENSED",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/index.d.ts",
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "",
    "dev": "",
    "build": "rm -rf ./dist && tsc -b tsconfig.cjs.json tsconfig.esm.json tsconfig.types.json",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:types": "tsc -p tsconfig.types.json",
    "build:rollup": "rm -rf ./dist && npx rollup -c npx lerna run build:types --scope=@brincat/vue",
    "test": "npx lerna run build --scope=@brincat/vue && jest"
  },
  "dependencies": {
    "@brincat/core": "*"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.1.6",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vitejs/plugin-vue-jsx": "^3.1.0",
    "jest": "^29.7.0",
    "postcss": "^8.4.35",
    "rollup": "^4.12.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "typescript": "^5.3.3"
  }
}
